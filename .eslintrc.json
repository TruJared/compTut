{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "class-methods-use-this": [
      2,
      {
        "exceptMethods": ["attributeChangedCallback"]
      }
    ],
    "consistent-return": 1,
    "no-underscore-dangle": 0,
    "no-console": 1,
    "no-useless-return": 0,
    "no-return-assign": 0,
    "no-unused-expressions": 1
  }
}
